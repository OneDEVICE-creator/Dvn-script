if game.PlaceId == 8657806289 then

    local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
    
    local Window = Rayfield:CreateWindow({
        Name = "Death Hub ðŸ˜¼",
        Theme = "Ocean",
        ConfigurationSaving = {
            Enabled = true,
            FileName = "Death Hub"
        }
    })
    
    local MainTab = Window:CreateTab("Main")
    local MainSection = MainTab:CreateSection("Dvn-Script")
    
    Rayfield:Notify({
        Title = "Script Executed",
        Content = "The script has been executed successfully!",
        Duration = 6.5,
    })
    
    -- Gun Mod Button
    local MainButton = MainTab:CreateButton({
        Name = "Gun Mod Gui",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/OneDEVICE-creator/Dvn-script/main/Gun%20mod"))()
        end,
    })
    
    -- Sliders for Head Size and Transparency
    local MainSlider1 = MainTab:CreateSlider({
        Name = "Enemy Head Size",
        Range = {1, 30},
        Increment = 0.5,
        CurrentValue = _G.EnemiesHeadSize or 8.0,
        Callback = function(Value)
            _G.EnemiesHeadSize = Value
        end,
    })
    
    local MainSlider2 = MainTab:CreateSlider({
        Name = "Prometheus Head Size",
        Range = {1, 10},
        Increment = 0.5,
        CurrentValue = _G.PrometheusHeadSize or 4.0,
        Callback = function(Value)
            _G.PrometheusHeadSize = Value
        end,
    })
    
    local MainSlider3 = MainTab:CreateSlider({
        Name = "Transparency",
        Range = {0, 1},
        Increment = 0.1,
        CurrentValue = _G.Transparency or 0.9,
        Callback = function(Value)
            _G.Transparency = Value
        end,
    })
    
    -- Toggle for Big Head Script
    local Toggle = MainTab:CreateToggle({
        Name = "Enable Big Heads",
        CurrentValue = false,
        Callback = function(Value)
            _G.ScriptEnabled = Value
            
            if Value then
                -- Load the Big Head script
                loadstring(game:HttpGet("https://raw.githubusercontent.com/OneDEVICE-creator/Dvn-script/main/Bighead%20for%20enemy"))()
                
                Rayfield:Notify({
                    Title = "Big Heads",
                    Content = "Big Heads Enabled!",
                    Duration = 3,
                })
            else
                -- Reset enemy heads
                for _, v in ipairs(workspace:GetChildren()) do
                    if v:IsA("Model") and v:FindFirstChild("Head") and not v:FindFirstChildOfClass("Humanoid") then
                        local head = v.Head
                        head.Size = Vector3.new(1, 1, 1)
                        head.Transparency = 0
                    end
                end

                Rayfield:Notify({
                    Title = "Big Heads",
                    Content = "Big Heads Disabled!",
                    Duration = 3,
                })
            end
        end,
    })
    
end
